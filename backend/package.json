        "react-router-dom": "^7.2.0",
        "resend": "4.0.1",
        "ulid": "^2.3.0",
        "zod": "3.22.4"
    },
    "devDependencies": {
        "@medusajs/test-utils": "latest",
        "@mikro-orm/cli": "6.4.3",
        "@swc/core": "1.5.7",
        "@swc/jest": "^0.2.37",
        "@tanstack/react-query": "5.64.2",
