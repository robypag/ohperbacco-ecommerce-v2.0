{
    "name": "ohperbacco-ecommerce-v2.0",
    "version": "0.0.2",
    "description": "A Wine e-commerce based on Medusa 2.0",
    "author": "robypag",
    "license": "MIT",
    "keywords": [
        "sqlite",
        "postgres",
        "typescript",
        "ecommerce",
        "headless",
        "medusa"
    ],
    "scripts": {
        "build": "medusa build && node src/scripts/postBuild.js",
        "seed": "medusa exec ./src/scripts/seed.ts",
        "ib": "init-backend",
        "start": "init-backend && cd .medusa/server && medusa start --verbose",
        "dev": "medusa develop",
        "email:dev": "email dev --dir=./src/modules/email-notifications/templates --port=3002"
    },
    "dependencies": {
        "@ai-sdk/openai": "^1.1.14",
